{
	"electron": {
		"description": "Launch Electron desktop application",
		"examples": [
			"electron(commandRoot=\"./command-plugins\", projectRoot=\".\")",
			"electron(commandRoot=\"./command-plugins\", projectRoot=\".\", servedui=\"./dist\")"
		],
		"parameters": {
			"commandRoot": {
				"description": "Root directory for command manifests"
			},
			"projectRoot": {
				"description": "Root directory for the project"
			},
			"servedui": {
				"description": "UI directory to load in Electron window"
			},
			"electronPreloadPath": {
				"description": "Path to Electron preload script"
			}
		}
	}
}
