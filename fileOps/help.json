{
  "getModel": {
    "description": "Read and parse a JSON model file",
    "syntax": "getModel(path)",
    "examples": [
      "getModel(\"./data/models/my_model.json\")",
      "getModel({path: \"models/story.json\"})"
    ],
    "parameters": {
      "path": {
        "description": "Path to JSON file relative to project root"
      }
    }
  },
  "saveToCorpus": {
    "description": "Save buffer data to corpus directory",
    "syntax": "saveToCorpus(buffer, filename)",
    "examples": [
      "saveToCorpus(bufferData, \"my_corpus.txt\")"
    ],
    "parameters": {
      "buffer": {
        "description": "Buffer data to save"
      },
      "filename": {
        "description": "Filename to save as"
      }
    }
  },
  "getCorpus": {
    "name": "getCorpus",
    "methodName": "returnTextFromFile",
    "combineArguments": true,
    "parameters": {
      "path": {
        "type": "string", 
        "required": true,
        "description": "Path to text file relative to project root"
      }
    }
  },
  "saveToModels": {
    "name": "saveToModels", 
    "methodName": "saveObjectToModels",
    "combineArguments": true,
    "parameters": {
      "data": {
        "type": "object",
        "required": true,
        "description": "Model data object to save"
      },
      "filename": {
        "type": "string",
        "required": true, 
        "description": "Model filename to save as"
      }
    }
  }
}