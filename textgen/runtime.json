{
  "generate": {
    "sideEffects": {
      "setState": {
        "currentModel": {
          "fromParam": "modelName"
        }
      }
    },
    "parameters": {
      "modelName": {
        "runtimeFallback": "currentModel"
      },
      "length": {
        "default": 100,
        "min": 1
      },
      "min_tokens": {
        "default": 50,
        "min": 1
      },
      "temperature": {
        "default": 1,
        "min": 0,
        "max": 2
      },
      "stop": {
        "default": [
          ".",
          "!",
          "?"
        ]
      },
      "samples": {
        "default": 1,
        "min": 1
      },
      "allowRepetition": {
        "default": true
      }
    }
  },
  "train": {
    "sideEffects": {
      "setState": {
        "currentModel": {
          "fromParam": "modelName",
          "template": "{{file | basename}}.json"
        }
      }
    },
    "parameters": {
      "file": {
        "runtimeFallback": "defaultCorpus"
      },
      "modelType": {
        "enum": [
          "markov",
          "vlmm",
          "hmm"
        ]
      },
      "order": {
        "default": 2,
        "min": 1,
        "max": 10
      },
      "modelName": {
        "kind": "implicit"
      },
      "caseSensitive": {
        "default": false
      },
      "trackStartStates": {
        "default": true
      }
    }
  }
}