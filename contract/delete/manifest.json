{
  "name": "delete",
  "description": "Delete a saved model",
  "syntax": "delete(modelName)",
  "parameters": [
    {
      "name": "modelName",
      "type": "string",
      "required": true,
      "description": "Name of the model file to delete"
    }
  ],
  "sideEffects": {
    "clearStateIf": {
      "currentModel": { "fromParam": "modelName" }
    }
  },
  "examples": ["delete(\"old_model.json\")"]
}