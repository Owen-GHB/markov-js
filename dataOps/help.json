{
  "delete": {
    "description": "Delete a saved model",
    "syntax": "delete(modelName)",
    "examples": [
      "delete(\"old_model.json\")"
    ],
    "parameters": {
      "modelName": {
        "description": "Name of the model file to delete"
      }
    }
  },
  "listCorpus": {
    "description": "List all available corpus files",
    "syntax": "listCorpus()",
    "examples": [
      "listCorpus()"
    ]
  },
  "listModels": {
    "description": "List all available saved models",
    "syntax": "listModels()",
    "examples": [
      "listModels()"
    ]
  },
  "uploadFile": {
    "description": "Upload a text file to the corpus directory",
    "syntax": "uploadFile(file, [filename])",
    "examples": [
      "uploadFile(fileObject)",
      "uploadFile(fileObject, \"my_corpus.txt\")"
    ],
    "parameters": {
      "file": {
        "description": "Text file to upload (from file picker or upload)"
      },
      "filename": {
        "description": "Optional custom filename (uses original name if not provided)"
      }
    }
  }
}